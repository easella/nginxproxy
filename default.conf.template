
server {
   proxy_cache_path /data/nginx/cache keys_zone=one:10m;
  proxy_cache mycache;
  listen $PORT default_server;

  location /games {
    root   /usr/share/nginx/html;
    index  index.html index.htm;
  },
   location / {
    root   /usr/share/nginx/html;
    index  index.html algebra.htm;
  }
}
